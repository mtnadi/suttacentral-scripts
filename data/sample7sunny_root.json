{
  "pli-tv-pvr7:0.1": "Parivāra",
  "pli-tv-pvr7:0.2": "Ekuttarikanaya",
  "pli-tv-pvr7:0.3": "1. Ekakavāra",
  "pli-tv-pvr7:112.5": "8. Aṭṭhakavāra",
  "pli-tv-pvr7:113.1": "Aṭṭhānisaṃse sampassamānena na so bhikkhu āpattiyā adassane ukkhipitabbo.",
  "pli-tv-pvr7:113.2": "Aṭṭhānisaṃse sampassamānena paresampi saddhāya sā āpatti desetabbā.",
  "pli-tv-pvr7:113.3": "Aṭṭha yāvatatiyakā.",
  "pli-tv-pvr7:113.4": "Aṭṭhahākārehi kulāni dūseti.",
  "pli-tv-pvr7:113.5": "Aṭṭha mātikā cīvarassa uppādāya.",
  "pli-tv-pvr7:113.6": "Aṭṭha mātikā kathinassa ubbhārāya.",
  "pli-tv-pvr7:113.7": "Aṭṭha pānāni.",
  "pli-tv-pvr7:113.8": "Aṭṭhahi asaddhammehi abhibhūto pariyādinnacitto devadatto āpāyiko nerayiko kappaṭṭho atekiccho.",
  "pli-tv-pvr7:113.9": "Aṭṭha lokadhammā.",
  "pli-tv-pvr7:113.10": "Aṭṭha garudhammā.",
  "pli-tv-pvr7:113.11": "Aṭṭha pāṭidesanīyā.",
  "pli-tv-pvr7:113.12": "Aṭṭhaṅgiko musāvādo.",
  "pli-tv-pvr7:113.13": "Aṭṭha uposathaṅgāni.",
  "pli-tv-pvr7:113.14": "Aṭṭha dūteyyaṅgāni.",
  "pli-tv-pvr7:113.15": "Aṭṭha titthiyavattāni.",
  "pli-tv-pvr7:113.16": "Aṭṭha acchariyā abbhutadhammā mahāsamudde.",
  "pli-tv-pvr7:113.17": "Aṭṭha acchariyā abbhutadhammā imasmiṃ dhammavinaye.",
  "pli-tv-pvr7:113.18": "Aṭṭha anatirittā.",
  "pli-tv-pvr7:113.19": "Aṭṭha atirittā.",
  "pli-tv-pvr7:113.20": "Aṭṭhame aruṇuggamane nissaggiyaṃ hoti.",
  "pli-tv-pvr7:113.21": "Aṭṭha pārājikā.",
  "pli-tv-pvr7:113.22": "Aṭṭhamaṃ vatthuṃ paripūrentī nāsetabbā.",
  "pli-tv-pvr7:113.23": "Aṭṭhamaṃ vatthuṃ paripūrentiyā desitāpi adesitā hoti.",
  "pli-tv-pvr7:113.24": "Aṭṭhavācikā upasampadā.",
  "pli-tv-pvr7:113.25": "Aṭṭhannaṃ paccuṭṭhātabbaṃ.",
  "pli-tv-pvr7:113.26": "Aṭṭhannaṃ āsanaṃ dātabbaṃ.",
  "pli-tv-pvr7:113.27": "Upāsikā aṭṭha varāni yācati.",
  "pli-tv-pvr7:113.28": "Aṭṭhahaṅgehi samannāgato bhikkhu bhikkhunovādako sammannitabbo.",
  "pli-tv-pvr7:113.29": "Aṭṭhānisaṃsā vinayadhare.",
  "pli-tv-pvr7:113.30": "Aṭṭha paramāni.",
  "pli-tv-pvr7:113.31": "Tassapāpiyasikākammakatena bhikkhunā aṭṭhasu dhammesu sammā vattitabbaṃ.",
  "pli-tv-pvr7:113.32": "Aṭṭha adhammikāni pātimokkhaṭṭhapanāni aṭṭha dhammikāni pātimokkhaṭṭhapanānīti.",
  "pli-tv-pvr7:114.1": "Aṭṭhakaṃ niṭṭhitaṃ.",
}
